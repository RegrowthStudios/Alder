{
    "name": "regrowthstudios/alder_public_authentication",
    "description": "The public user authentication service for Alder, a service package for game developers from Regrowth Studios.",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "Matthew Marshall",
            "email": "matthew.marshall96@yahoo.co.uk"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "roave/security-advisories": "dev-master",
        "zendframework/zend-expressive": "^1.0",
        "zendframework/zend-expressive-helpers": "^2.0",
        "zendframework/zend-stdlib": "^2.7 || ^3.0",
        "zendframework/zend-expressive-fastroute": "^1.0",
        "zendframework/zend-servicemanager": "^2.7.3 || ^3.0",
        "ocramius/proxy-manager": "^1.0 || ^2.0",
        "lcobucci/jwt": "^3.1",
        "mtymek/expressive-config-manager": "^0.4.0",
        "zendframework/zend-json": "^3.0",
        "zendframework/zend-diactoros": "^1.3",
        "zendframework/zend-log": "^2.9",
        "zendframework/zend-math": "^3.0",
        "zendframework/zend-db": "^2.8",
        "zendframework/zend-cache": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "squizlabs/php_codesniffer": "^2.3",
        "filp/whoops": "^1.1 || ^2.0"
    },
    "autoload": {
        "psr-4": {
            "Alder\\": "src/Alder/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlderTest\\": "tests/",
            "AlderTest\\Alder\\": [ "tests/Alder/unit/", "tests/Alder/functional/" ]
        }
    }
}
