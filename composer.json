{
    "name": "regrowthstudios/alder_core",
    "description": "The core library for all Alder services, a service package for game developed from Regrowth Studios.",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "Matthew Marshall",
            "email": "matthew.marshall96@yahoo.co.uk"
        }
    ],
    "require": {
        "php": "^7.0",
        "zendframework/zend-stdlib": "^3.1",
        "zendframework/zend-expressive": "^1.0",
        "zendframework/zend-permissions-acl": "^2.6",
        "zendframework/zend-json": "^3.0",
        "zendframework/zend-expressive-helpers": "^2.1",
        "mtymek/expressive-config-manager": "^0.4.0",
        "zendframework/zend-cache": "^2.7",
        "zendframework/zend-expressive-fastroute": "^1.2",
        "zendframework/zend-db": "^2.8",
        "zendframework/zend-math": "^3.0",
        "lcobucci/jwt": "^3.2",
        "zendframework/zend-log": "^2.9",
        "roave/security-advisories": "dev-master",
        "zendframework/zend-i18n": "^2.7",
        "adambrett/shell-wrapper": "^0.7.0",
        "symfony/filesystem": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "Alder\\": "src/Alder/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlderTest\\": "tests/",
            "AlderTest\\Alder\\": [ "tests/Alder/unit/", "tests/Alder/functional/" ]
        }
    }
}
